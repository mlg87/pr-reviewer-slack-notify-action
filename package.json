    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@types/node": "^16.10.5",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.31.1",
    "commitizen": "^4.2.3",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^8.5.0",
